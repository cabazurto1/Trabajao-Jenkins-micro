# Puerto para el frontend
FRONTEND_PORT=80

# Endpoints del frontend (Variables React)
REACT_APP_BACKEND_CURSOS=http://dasdas:${PORT_MICRO_CURSO}
REACT_APP_BACKEND_ESTUDIANTES=http://dasdasd:${PORT_MICRO_ESTUDIANTE}

CORS_ALLOWED_ORIGINS=*
